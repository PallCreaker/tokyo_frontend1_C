<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- <%= csrf_meta_tags %> -->
    <script type="text/javascript" src="/assets/common/main/mainpage.js"></script>
</head>
<body>
    <!-- Status bar overlay for fullscreen mode-->
    <div class="statusbar-overlay"></div>
    <div class="panel-overlay"></div>
    <div class="views">
        <!-- Main view has "view-main" class-->
        <div class="view view-main">
            <!-- Top Navbar-->
            <div class="navbar">
                <div class="navbar-inner">
                    <div class="left">
                        <span class="preloader"></span>
                    </div>
                    <div class="center sliding">
                        回答する人を探す
                    </div>
                    <div class="right">
                        <a href="#" class="link icon-only open-panel">
                            <i class="icon icon-bars-blue"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="pages navbar-through toolbar-through">
                <!-- For dynamic page rendering -->
                <div data-page="main" class="page">
                    <!-- Scrollable page content-->
                    <div class="page-content">
                        <a href="matching/3" id="main-page"></a>
                    </div>
                </div>
            </div>
            <!-- Bottom Toolbar Begin -->
            <div class="toolbar tabbar ">
                <div class="toolbar-inner">
                    <a href="#recommend" id="recommend" class="tab-link active">
                        <i class="icon demo-icon-1"></i>
                        <!-- <span class="tabbar-label">Recommend</span> -->
                    </a>
                    <a href="#notification" id="notification" class="tab-link active">
                        <i class="icon demo-icon-1"></i>
                        <!-- <span class="tabbar-label">Notifications</span> -->
                    </a>
                    <a href="#search" id="search" class="tab-link active">
                        <i class="icon demo-icon-1"></i>
                        <!-- <span class="tabbar-label">Search</span> -->
                    </a>
                    <a href="#question" id="question" class="tab-link active">
                        <i class="icon demo-icon-1"></i>
                        <!-- <span class="tabbar-label">Questions</span> -->
                    </a>
                    <a href="#mypage" id="mypage" class="tab-link active">
                        <i class="icon"></i>
                        <!-- <span class="tabbar-label">My page</span> -->
                    </a>
                </div>
            </div>
            <!-- Bottom Toolbar End -->
        </div>
    </div>

    <script type="text/javascript" src="/assets/fw7/framework7.min.js"></script>
    <script type="text/javascript" src="/assets/myapps/mainpage-app.js"></script>
</body>
</html>